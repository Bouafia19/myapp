            <li><a href="<%= root_path(locale: :en) %>"><%= image_tag ("img/united-states.png"), style: 'height:20%; width:20%;'%>&ensp;<%= t 'main_nav.language.english'%></a></li>
            <li><a href="<%= root_path(locale: :fr) %>"><%= image_tag ("img/france.png"), style: 'height:20%; width:20%;'%>&ensp;<%= t 'main_nav.language.frensh'%>&nbsp;</a></li>
            <li><a href="<%= root_path(locale: :ar) %>"><%= image_tag ("img/algeria.png"), style: 'height:20%; width:20%;'%>&ensp;<%= t 'main_nav.language.arabic'%>&nbsp;</a></li>
            
          </ul>
        </li>  
        
      </ul>
    </nav><!-- .main-nav -->

  </div>
</header><!-- #header -->

<!--==========================
  Intro Section
============================-->
<section id="intro" class="clearfix">
  <div class="container">

    <div class="intro-img">
      <%= image_tag ("img/undraw_Outer_space_re_u9vd.svg"), class: "img-fluid", style: 'height:80%; width:90%;'%>
    </div>

    <div class="intro-info">
      <h2><%= t 'intro_info.provide'%><br><span><%= t 'intro_info.solution'%></span><br><%= t 'intro_info.business'%></h2>
      <div>
