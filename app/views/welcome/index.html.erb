  <section id="contact">
    <div class="container-fluid">

      <div class="section-header">
        <h3><%= t 'section_contact.h3'%></h3>
      </div>

      <div class="row wow fadeInUp">

        <div class="col-lg-6">
          <div class="map mb-4 mb-lg-0" >
           <div id="map"></div>
              <%# <iframe src="https://www.google.com/maps/embed?pb=!1m14!1m12!1m3!1d7679.491781824357!2d5.438146650808376!3d36.20668047768116!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!5e1!3m2!1sen!2sbg!4v1592563618870!5m2!1sen!2sbg" frameborder="0" style="border:0; width: 100%; height: 312px;" allowfullscreen></iframe> %>
          </div>
        </div>
        <div class="col-lg-6">
          <div class="row">
            <div class="col-md-5 info">
              <i class="ion-ios-location-outline" style="color:#00366f;"></i>
              <p><%= t 'section_contact.info.location'%></p>
            </div>
            <div class="col-md-4 info">
              <i class="ion-ios-email-outline" style="color:#00366f;"></i>
              <p>contact@nextchallenge-dz.com</p>
            </div>
            <div class="col-md-3 info">
              <i class="ion-ios-telephone-outline" style="color:#00366f;"></i>
              <p>+213 552 56 96 54</p>
            </div>
          </div>

          <div class="form">
            <div id="sendmessage">Your message has been sent. Thank you!</div>
            <div id="errormessage">Sorry there is a problem</div>
            <form action="https://formspree.io/f/mvoddrqe" method="POST" role="form" class="contactForm">
            <%#= form_for @contact, html: { class: "contactForm" } do |f| %>
              <div class="form-row">
                <div class="form-group col-lg-6">
                  <input type="text" name="name" class="form-control" id="name" placeholder="<%= t 'section_contact.form.name'%>" data-rule="minlen:4" data-msg="Please enter at least 4 chars" />
                  <%#= f.text_field :name, placeholder: "#{t('section_contact.form.name')}", class: "form-control" %>
                  <div class="validation"></div>
                </div>
                <div class="form-group col-lg-6">
                  <input type="email" class="form-control" name="_replyto" id="email" placeholder="<%= t 'section_contact.form.email'%>" data-rule="email" data-msg="Please enter a valid email" />
                  <%#= f.text_field :email, placeholder: "#{t('section_contact.form.email')}", class: "form-control" %>
                  <div class="validation"></div>
                </div>
              </div>
              <div class="form-group">
                <input type="text" class="form-control" name="subject" id="message" placeholder="<%= t 'section_contact.form.subject'%>" data-rule="minlen:4" data-msg="Please enter at least 8 chars of subject" />
                <%#= f.text_field :subject, placeholder: "#{t('section_contact.form.subject')}", class: "form-control" %>
                <div class="validation"></div>
              </div>
              
              <div class="form-group">
                <textarea class="form-control" name="message" rows="5" data-rule="required" data-msg="Please write something for us" placeholder="<%= t 'section_contact.form.message'%>"></textarea>
                <%#= f.text_area :message, placeholder: "#{t('section_contact.form.message')}" ,class: 'form-control', rows: "5" %>
                <div class="validation"></div>
              </div>
              <div class="form-group">
                <%#= recaptcha_tags %>
                 <%#= f.hidden_field :nickname, class: 'form-control' %>
              </div>
              <div class="text-center"><button id="sendMessageButton" type="submit" title="Send Message"><%= t 'section_contact.form.button'%></button></div>
              <div class="text-center">
                <%#= f.submit "#{t('section_contact.form.button')}", class: "btn btn-primary" %>
              </div>
            </form>
            <%# end %>
          </div>
        </div>

      </div>

    </div>
  </section><!-- #contact -->

