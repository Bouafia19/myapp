<body>

<!--==========================
Header
============================-->
<header id="header" class="fixed-top">
  <div class="container">

    <div class="logo float-left">
      <!-- Uncomment below if you prefer to use an image logo -->
      <h1 class="text-light"><a href="#header"><span><%= t 'header'%></span></a></h1> 
      <%# <a href="#intro" class="scrollto">= image_tag ("img/logo1.png"), class: "img-fluid"</a> %>
    </div>

    <nav class="main-nav float-right d-none d-lg-block">
      <ul>
        <li class="active"><a href="#intro"><%= t 'main_nav.home'%></a></li>
        <li><a href="#about"><%= t 'main_nav.about_us'%></a></li>
        <li><a href="#services"><%= t 'main_nav.services'%></a></li>
        <li><a href="#portfolio"><%= t 'main_nav.portfolio'%></a></li>
        <li><a href="#team"><%= t 'main_nav.technologies'%></a></li>
        <li><a href="#contact"><%= t 'main_nav.contact_us'%> &emsp; </a></li>

        
